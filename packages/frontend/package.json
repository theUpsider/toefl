{
  "name": "@package/frontend",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "server": "npx ts-node ./server/src/server.ts",
    "build": "tsc && vite build",
    "serve": "vite preview --host",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix",
    "lint:format": "prettier  --log-level warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix ",
    "type-check": "tsc",
    "test": "jest"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.1.0",
    "@package/lib": "workspace:*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "react-use-websocket": "^4.10.1",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^5.0.7",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "vite": "^4.5.5",
    "vite-plugin-pwa": "^0.20.5",
    "vite-tsconfig-paths": "^4.3.2",
    "workbox-build": "^7.3.0",
    "workbox-window": "^7.3.0"
  },
  "pre-commit": "lint",
  "license": "MIT"
}
